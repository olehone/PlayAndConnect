@{
    string[] imageNames = new[] { "image1.jpg", "image2.jpg", "image3.jpg", "image4.jpg", "image5.jpg", "image6.jpg", "image7.jpg", "image8.jpg", "image9.jpg", "image10.jpg" };
}

<ul>
    @foreach (var imageName in imageNames)
    {
        <li>
            <a href="@Url.Action("ShowImage", "HomeController", new { imageName = imageName })">
                @imageName
            </a>
        </li>
    }
</ul>
